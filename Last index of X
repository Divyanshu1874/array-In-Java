//Write a program to find x

public class practiceRecursion {


    public static int cLast( int x, int arr[]) {



        for (int i = arr.length-1; i >= 0; i--) {

            if (arr[i] == x) {
                return i;
            }

        } return -1;
    }




    public static void main(String[] args) {

        int arr[] = {8, 8, 4, 8, 6, 8};
        int x = 8;

        int result = cLast(x, arr);
        System.out.println(result);


    }
}
