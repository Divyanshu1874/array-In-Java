  public boolean containsDuplicate(int[] nums) {
	 /*     1 2 3 1 This is element
	  after arrays.sort method element will be 1 1 2 3 * */
	        Arrays.sort(nums);    //This is function to sort Arrays
	        for (int i = 1; i < nums.length; i++) {
	            if (nums[i] == nums[i - 1]) {
	              return true;
	            }
	        }return false;
	    }
	}
	
	     
